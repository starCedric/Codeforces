// solution : 

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int date;
	cin >> date;
	
	if(date == 8) cout << "YES";
	
	else if((date / 8) != 0)
	    cout << "NO";
	   
	else cout << "YES";
	
	return 0;

}
